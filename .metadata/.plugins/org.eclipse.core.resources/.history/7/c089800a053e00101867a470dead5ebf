package com.features;
@FunctionalInterface
interface Filter{
	public boolean filt(int x);
}
class ArrayFilter{
	public void arrEle(int arr[],Filter obj) {
		for(int a:arr) {
			if(obj.filt(a)) {
				System.out.println(a);
			}
		}
	}
}
public class filterExample {
	public static void main(String[] args) {
	
	}
}
